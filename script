<script>
  var ua=navigator.userAgent||"", IOS=/iPhone|iPad|iPod/i.test(ua), AND=/Android/i.test(ua);
  var IOS_URL="https://apps.apple.com/us/app/bent-tree-for-patients/id1617296296";
  var AND_URL="https://play.google.com/store/apps/details?id=health.bentree.patient&pcampaignid";
  function go(u){ try{ top.location.href=u; }catch(e){ location.replace(u); } }
  if(IOS) go(IOS_URL); else if(AND) go(AND_URL);
  else { s.textContent="Couldn't detect a phone."; f.style.display="block"; }
</script>
